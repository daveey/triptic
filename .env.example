# Triptic Environment Variables
# Copy this file to .env and fill in your values

# Gemini API Key (required for AI image generation)
GEMINI_API_KEY=your_gemini_api_key_here

# Authentication credentials for web interface (required)
# These protect access to management pages like wall.html, playlists.html, etc.
TRIPTIC_AUTH_USERNAME=your_username_here
TRIPTIC_AUTH_PASSWORD=your_secure_password_here
